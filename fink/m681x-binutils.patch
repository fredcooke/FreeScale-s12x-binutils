+                0x00ff,		/* src_mask */
+                0x00ff,		/* dst_mask */
+                FALSE),		/* pcrel_offset */
+
+                /* A 8 bit absolute relocation (upper address) */
+                HOWTO (R_MC9XGATE_LO8,		/* type */
+                    0,			/* rightshift */
+                    0,			/* size (0 = byte, 1 = short, 2 = long) */
+                    8,			/* bitsize */
+                    FALSE,			/* pc_relative */

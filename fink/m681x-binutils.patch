+#
+# Some generic m68HC11 tests
+#
+if ![istarget "m68hc11-*-*"] then {
+    if ![istarget "m6811-*-*"] then {
+	if ![istarget "m68hc12-*-*"] then {
+	    if ![istarget "m6812-*-*"] then {
+		return
+	    }
+	}
+    }
+}

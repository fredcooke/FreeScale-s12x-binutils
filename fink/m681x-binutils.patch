+	std	[d,y]
+	std	[d,sp]
+	std	[d,pc]
+	beq	L1
+	lbeq	start
+	lbcc	L2
+;;
+;; Move insn with various operands
+;; 
+	movb	start, 1,x
+	movw	1,x, start
+	movb	start, 1,+x
+	movb	start, 1,-x
+	movb	#23, 1,-sp
+	movb	L1, L2
+	movb	L1, a,x
+	movw	L1, b,x
+	movw	L1, d,x
+	movw	d,x, a,x
+	movw	b,sp, d,pc
